{
  "name": "expo-updates-server",
  "type": "module",
  "scripts": {
    "dev": "npm run rakkas:dev",
    "generate-manifest": "node -r esbuild-register src/cli/generate-manifest.ts ../expo/dist 0.0.0",
    "build": "npm run rakkas:build:vercel",
    "deploy": "npm run rakkas:build:vercel && vercel deploy --prebuilt . --prod",
    "rakkas:dev": "rakkas --no-clearScreen --port 3000",
    "rakkas:build": "rakkas build",
    "rakkas:build:vercel": "CONFIG_RAKKAS_ADAPTER=vercel npm run rakkas:build"
  },
  "dependencies": {
    "@hattip/response": "^0.0.17",
    "rakkasjs": "^0.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "^16",
    "esbuild": "^0.15.9",
    "esbuild-register": "^3.3.3",
    "undici": "^5.10.0",
    "vite": "^3.1.3"
  }
}
